{
  "name": "benarid-chrome-extension",
  "version": "0.2.1",
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "bsb -make-world",
    "watch": "run-p watch:bs watch:js",
    "watch:bs": "bsb -make-world -w",
    "watch:js": "rollup -c -w",
    "bundle": "rollup -c",
    "pretest": "npm run build",
    "test": "jest ./lib/js/test/*test.js"
  },
  "keywords": [
    "BuckleScript"
  ],
  "license": "MIT",
  "devDependencies": {
    "bs-fetch": "^0.2.0",
    "bs-jest": "^0.2.0",
    "bs-platform": "^1.9.3",
    "bucklescript-tea": "^0.6.11",
    "npm-run-all": "^4.1.1",
    "rollup": "^0.50.0"
  },
  "dependencies": {},
  "jest": {
    "testRegex": "/*test.js$"
  }
}
